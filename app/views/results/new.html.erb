<h1>New Result</h1>
<%= simple_form_for :result, url: results_path do |f| %>
	<div class="form-group">
		<%= f.input :프로젝트명, input_html: { class: 'form-control' }, label: false, placeholder: '프로젝트명' %>
	</div>

	<div class="form-group">
		시작일자 <%= f.date_field :시작일자, input_html: { class: 'form-control' } %>
	</div>
	<div class="form-group">
		종료일자 <%= f.date_field :종료일자, input_html: { class: 'form-control' } %>
	</div>
	<div class="form-group">
		첨부파일 <%= f.file_field :attachment, input_html: { class: 'form-control' } %>
	</div>
	<div class="form-group">
		<%= f.input :소개, input_html: { class: 'form-control' }, label: false, placeholder: '프로젝트 소개' %>
	</div>
	<div class="form-group">
		<%= f.input :소감, input_html: { class: 'form-control' }, label: false, placeholder: '소감' %>
	</div>
	<div class="form-group">
		<%= f.button :submit, class: 'btn btn-default' %>
	</div>

<% end %>

<%= link_to 'Back', projects_path %>
