<h1>프로젝트 수정</h1>

<%= form_for :project, url: project_path(@project), method: :patch do |f| %>
 
 	<p>
		<%= f.label :프로젝트명 %><br>
		<%= f.text_field :프로젝트명 %>
	</p>

	<p>
		<%= f.label :일정 %><br>
		<%= f.text_area :일정 %>
	</p>

	<p>
		<%= f.label :목표 %><br>
		<%= f.text_area :목표 %>
	</p>
	<%= f.submit %>
<% end %>

<%= link_to 'Back', projects_path %>
