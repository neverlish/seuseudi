<h1>New project</h1>
<%= form_for :project, url: projects_path do |f| %>
	<p>
		<%= f.label :프로젝트명 %><br>
		<%= f.text_field :프로젝트명 %>
	</p>

	<p>
		<%= f.file_field :photo %>
	</p>

	<p>
		<%= f.label :시작일자 %><br>
		<%= f.date_field :시작일자 %>
	</p>

	<p>
		<%= f.label :종료일자 %><br>
		<%= f.date_field :종료일자 %>
	</p>
	<p>
		<%= f.label :일정 %><br>
		<%= f.text_area :일정 %>
	</p>

	<p>
		<%= f.label :목표 %><br>
		<%= f.text_area :목표 %>
	</p>
	<%= f.submit %>
<% end %>

<%= link_to 'Back', projects_path %>
